<view class="container">
  <view class="result-item">
    <text>总投资: {{result.totalInvestment}}</text>
  </view>
  <view class="result-item">
    <text>第一年净利润: {{result.netProfit}}</text>
  </view>
  <view class="result-item">
    <text>累计净利润: {{result.cumulativeNetProfit}}</text>
  </view>
  <view class="result-item">
    <text>资产回报率（ROI）: {{result.ROI}}</text>
  </view>
  <view class="result-item">
    <text>内部收益率（IRR）: {{result.IRR}}</text>
  </view>
  <view class="result-item">
    <text>平均毛利率（GP）: {{result.averageGP}}</text>
  </view>
  <view class="result-item conclusion">
    <text>项目总体结论：{{result.summaryConclusion}}</text>
  </view>
</view>
